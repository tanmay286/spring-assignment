<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:util="http://www.springframework.org/schema/util"
xmlns:context = "http://www.springframework.org/schema/context"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd
http://www.springframework.org/schema/context
      http://www.springframework.org/schema/context/spring-context.xsd
  http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">

<context:component-scan base-package="com"/>
<util:list id="directors">
           <value>directors1</value>
           <value>directors2</value>
           <value>directors3</value>
           <value>directors4</value>
      </util:list>
    <util:set id="branches">
           <value>branche1</value>
           <value>branche2</value>
           <value>branche3</value>
           <value>branche4</value>
      </util:set>
    <util:map id="branchHeaders">
            <entry key="branch1" value="director1" />
            <entry key="branch2" value="director2" />
            <entry key="branch3" value="director3" />
            <entry key="branch4" value="director4" />
        </util:map>
    
       
	<util:properties id = "ip" location="ipConfig.properties"/>   
        <util:properties id="database" location="dbConfig.properties"/> 


</beans>